{
  "name": "@tsed/platform-cache",
  "version": "7.0.0-alpha.2",
  "description": "Platform cache module for Ts.ED Framework",
  "private": false,
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn run build:esm && yarn run build:cjs",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json"
  },
  "dependencies": {
    "@types/cache-manager": "^3.4.2",
    "cache-manager": "^3.4.1",
    "micromatch": "^4.0.2",
    "tslib": "2.2.0"
  },
  "devDependencies": {
    "@tsed/core": "7.0.0-alpha.2",
    "@tsed/di": "7.0.0-alpha.2",
    "@tsed/exceptions": "7.0.0-alpha.2",
    "@tsed/json-mapper": "7.0.0-alpha.2",
    "@tsed/schema": "7.0.0-alpha.2",
    "@types/cache-manager": "^3.4.2",
    "@types/micromatch": "^4.0.1"
  },
  "peerDependencies": {
    "@tsed/core": "^7.0.0-alpha.2",
    "@tsed/di": "^7.0.0-alpha.2",
    "@tsed/exceptions": "^7.0.0-alpha.2",
    "@tsed/json-mapper": "^7.0.0-alpha.2",
    "@tsed/schema": "^7.0.0-alpha.2"
  }
}
